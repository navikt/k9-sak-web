{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2020",
    "moduleResolution": "node",
    "types": ["node", "jest", "vite/client"],
    "allowJs": true,
    "noEmit": true,
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "typeRoots": ["./node_modules/@types", "./@types"],
    "baseUrl": "packages/",
    "skipLibCheck": true //TODO Fjern denne
  },
  "include": ["packages/**/*", "@types", "_mocks"],
  "exclude": ["packages/**/cypress.config.ts", "packages/**/cypress"]
}
