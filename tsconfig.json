{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2020",
    "moduleResolution": "node",
    "types": [ "node", "jest", "@types/testing-library__jest-dom" ],
    "allowJs": true,
    "noEmit": true,
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "typeRoots": ["./node_modules/@types", "./@types"],
    "baseUrl": "packages/",
    "skipLibCheck": true //TODO Fjern denne
  },
  "include": [
		"packages/**/*",
		"@types"
	],
}
